public class Lab02a
{
   public static void main( String[] args )
   {
      // variables
      SimpleURLReader url = new SimpleURLReader( "http://www.cs.bilkent.edu.tr/~ozturk/cs102/housman.txt" );

      // program code
      System.out.println( url.getPageContents().substring(4) );
